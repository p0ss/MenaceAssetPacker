{
  "modkit": "1.0.0",
  "dependencies": {
    "MelonLoader": {
      "version": "0.7.2-ci.2388",
      "source": "https://github.com/LavaGang/MelonLoader/actions/runs/20586441562",
      "note": "Nightly build required for Unity 6 (6000.x) support",
      "bundledPath": "bundled/MelonLoader"
    },
    "DataExtractor": {
      "version": "4.6.0",
      "source": "local-build",
      "bundledPath": "bundled/DataExtractor"
    },
    "ModpackLoader": {
      "version": "2.0.0",
      "source": "local-build",
      "bundledPath": "bundled/ModpackLoader"
    },
    "AssetRipper": {
      "version": "1.3.4",
      "source": "https://github.com/AssetRipper/AssetRipper",
      "downloadUrls": {
        "linux": "https://github.com/AssetRipper/AssetRipper/releases/download/1.3.4/AssetRipper_linux_x64.zip",
        "windows": "https://github.com/AssetRipper/AssetRipper/releases/download/1.3.4/AssetRipper_win_x64.zip"
      },
      "bundledPath": "bundled/AssetRipper"
    },
    "TwitchServer": {
      "version": "1.0.0",
      "source": "local-build",
      "note": "Companion server for TwitchSquaddies modpack",
      "bundledPath": "bundled/tools/TwitchServer"
    },
    "DotNetRefs": {
      "version": "6.0.36",
      "source": "https://www.nuget.org/packages/Microsoft.NETCore.App.Ref/6.0.36",
      "note": "Bundled .NET 6 reference assemblies for modpack compilation. Required because MelonLoader runs on .NET 6, and compiling against newer runtimes causes assembly version mismatches.",
      "bundledPath": "bundled/dotnet-refs/net6.0"
    }
  }
}
